local function MLEWJ_fake_script() -- Misc_2.LocalScript 
	local script = Instance.new('LocalScript', Misc_2)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Main1.Visible = false
		script.Parent.Parent.Misc1.Visible = false
		script.Parent.Parent.Home1.Visible = false
		script.Parent.Parent.Credit1.Visible = true
		wait(0.1)
	end)
end
coroutine.wrap(MLEWJ_fake_script)()
