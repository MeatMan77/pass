-- Scripts:

local function TLEL_fake_script() -- Credit.LocalScript 
	local script = Instance.new('LocalScript', Credit)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Main1.Visible = false
		script.Parent.Parent.Misc1.Visible = false
		script.Parent.Parent.Home1.Visible = false
		script.Parent.Parent.Credit1.Visible = true
		wait(0.1)
	end)
end
coroutine.wrap(TLEL_fake_script)()
local function WPKZAUP_fake_script() -- Home.LocalScript 
	local script = Instance.new('LocalScript', Home)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Main1.Visible = false
		script.Parent.Parent.Misc1.Visible = false
		script.Parent.Parent.Home1.Visible = true
		script.Parent.Parent.Credit1.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(WPKZAUP_fake_script)()
local function HTDAB_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Main1.Visible = true
		script.Parent.Parent.Misc1.Visible = false
		script.Parent.Parent.Home1.Visible = false
		script.Parent.Parent.Credit1.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(HTDAB_fake_script)()
