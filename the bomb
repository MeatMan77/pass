hitbox.Name = "hitbox"
hitbox.Parent = Main1
hitbox.BackgroundColor3 = Color3.fromRGB(179, 0, 255)
hitbox.BackgroundTransparency = 1.000
hitbox.BorderSizePixel = 0
hitbox.Position = UDim2.new(0.0129032256, 0, 0.192886695, 0)
hitbox.Size = UDim2.new(0, 452, 0, 24)
hitbox.ZIndex = 2
hitbox.Font = Enum.Font.Highway
hitbox.Text = "Remove-hitbox(Click after every round)"
hitbox.TextColor3 = Color3.fromRGB(255, 255, 255)
hitbox.TextSize = 17.000
hitbox.TextXAlignment = Enum.TextXAlignment.Left
hitbox.MouseButton1Click:Connect(function()
	local LocalPlayer = game.Players.LocalPlayer
	local Character = LocalPlayer.Character

	for destructionIteration = 1, 100 do wait()
		pcall(function()
			Character:WaitForChild("CollisionPart"):Destroy()
			print("No More Hitbox")
		end)
	end

	LocalPlayer.CharacterAdded:Connect(function(Character)
		for destructionIteration = 1, 100 do wait()
			pcall(function()
				Character:WaitForChild("CollisionPart"):Destroy()
				print("No More Hitbox")
			end)
		end
	end)
end)




